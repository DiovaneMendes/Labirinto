package applications;

public class Sala{
	Porta puerta = new Porta();
	String north, south, east, west, up, down;
	int clickNorth, clickSouth, clickEast, clickWest, clickUp, clickDown;
	boolean saberInimigo;
	
	public Sala(String north, int clickNorth, String south, int clickSouth, String east, int clickEast, 
				String west, int clickWest, String up, int clickUp, String down, int clickDown, boolean saberInimigo){
		this.saberInimigo = saberInimigo;
		this.north = north;
		this.clickNorth = clickNorth;
		this.south = south;
		this.clickSouth = clickSouth;
		this.east = east;
		this.clickEast = clickEast;
		this.west = west;
		this.clickWest = clickWest;
		this.up = up;
		this.clickUp = clickUp;
		this.down = down;
		this.clickDown = clickDown;
	}
}