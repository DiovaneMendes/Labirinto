package model;

public class Sala{
	private int numeroSala;
	private String east, west, north, south, down, up;
	
	public Sala(int numeroSala, String east, String west, String north, String south, String down, String up){
		this.numeroSala = numeroSala;
		this.east = east;
		this.west = west;
		this.north = north;
		this.south = south;
		this.down = down;
		this.up = up;
	}
	
	@Override
    public String toString() {
        return "Room=" + numeroSala + ", Portas=" + porta;
    }
}