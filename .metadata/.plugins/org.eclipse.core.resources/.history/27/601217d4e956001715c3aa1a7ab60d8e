package applications;

public class Sala{
	Porta puerta = new Porta();
	String north, south, east, west, up, down;
	int clickNorth, clickSouth, clickEast, clickWest, clickUp, clickDown;
	int inimigoNorth, inimigoSouth, inimigoEast, inimigoWest, inimigoUp, inimigoDown;
	
	public Sala(String north, int clickNorth, int inimigoNorth, String south, int clickSouth, int inimigoSouth,
				String east, int clickEast, int inimigoEast, String west, int clickWest, int inimigoWest,
				String up, int clickUp, int inimigoUp, String down, int clickDown, int inimigoDown){
		this.north = north;
		this.clickNorth = clickNorth;
		this.south = south;
		this.clickSouth = clickSouth;
		this.east = east;
		this.clickEast = clickEast;
		this.west = west;
		this.clickWest = clickWest;
		this.up = up;
		this.clickUp = clickUp;
		this.down = down;
		this.clickDown = clickDown;
	}
}