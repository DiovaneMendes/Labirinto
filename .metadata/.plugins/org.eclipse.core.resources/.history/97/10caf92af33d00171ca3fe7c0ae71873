
public class Inimigo {
	private int danoI, acertoI;
	private boolean armado = false;
	
	public Inimigo(){
		
	}
	
	public void goblin(){
		danoI = 2;
		acertoI = 80;
		if(armado){
			acertoI =+ acertoI;
		}
	}
	
	public void orc(){
		danoI = 4;
		acertoI = 75;
	}
	
	public void troll(){
		danoI = 6;
		acertoI = 50;
	}
}
