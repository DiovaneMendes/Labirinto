package model;

public class Sala{
	private int numeroSala;
	private Vetor <Porta> porta;
	
	public Sala(int numeroSala, Vetor <Porta> porta){
		this.numeroSala = numeroSala;
		this.porta = porta;
	}
	
	public int getNumeroSala(){
		return numeroSala;
	}
	
	public void setNumeroSala(int numeroSala){
		this.numeroSala = numeroSala;
	}
}